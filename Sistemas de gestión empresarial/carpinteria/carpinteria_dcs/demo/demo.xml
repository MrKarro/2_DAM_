<odoo>
     <data>

          <record id="material_roble" model="carpinteria.material">
             <field name="name">Roble</field>
             <field name="stock">50</field>
             <field name="precio">80</field>
              <field name="imagen" type="base64" file="carpinteria_dcs/static/img/roble.jpg"/>
             <field name="disponible" eval="True"/>
             <field name="tipo">Ro</field>
         </record>


         <record id="producto_mesa_roble" model="carpinteria.producto">
             <field name="name">Mesa de Roble</field>
